package JETS;

import java.time.Duration;

import org.openqa.selenium.By;
import org.openqa.selenium.JavascriptExecutor;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.edge.EdgeDriver;

public class JSTS {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		WebDriver driver=new EdgeDriver();
		driver.manage().window().maximize();
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(2000));
		JavascriptExecutor jse= (JavascriptExecutor) driver;
		String url="https://www.facebook.com/";
		jse.executeScript("window.location=arguments[0];", url);
		WebElement un =driver.findElement(By.xpath("//input[@type='text']"));
		String un1="admin1";
		jse.executeScript("arguments[0].value=arguments[1];", un,un1);
		Thread.sleep(2000);
		WebElement pwd=driver.findElement(By.xpath("//input[@type='password']"));
		String pass="admin1234784348";
		jse.executeScript("arguments[0].value=arguments[1];", pwd,pass);
		Thread.sleep(2000);
		WebElement log=driver.findElement(By.xpath("//span[text()='Log in']"));
		jse.executeScript("arguments[0].click();", log);
		Thread.sleep(2000);
		driver.quit();	
	}
}
